<Window x:Class="WpfApplication1.SubWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApplication1"
        mc:Ignorable="d"
        Title="SubWindow" Height="720.3" Width="893">
    <Grid Height="350" Width="625" >
        <Grid HorizontalAlignment="Left" Height="310" VerticalAlignment="Top" Width="615" Margin="10,0,0,0">
            <DataGrid AutoGenerateColumns="False" HorizontalAlignment="Left" Margin="-75,-139,0,0"
Name="dataGridResult" Width="652" ItemsSource="{Binding Path=LoadDataBinding}"
CanUserResizeRows="False">
                <DataGrid.Columns>
                    <DataGridTextColumn Binding="{Binding Path=csv_id}" Header="ID" Width="*"
IsReadOnly="True" />
                    <DataGridTextColumn Binding="{Binding Path=csv_startup}" Header="Start Up" Width="*"
IsReadOnly="True" />
                    <DataGridTextColumn Binding="{Binding Path=csv_status}" Header="Status" Width="*"
IsReadOnly="True" />
                    <DataGridTextColumn Binding="{Binding Path=csv_service}" Header="Service" Width="*" IsReadOnly="True" />
                    <DataGridTextColumn Binding="{Binding Path=csv_subservice}" Header="Sub Service" Width="*"
IsReadOnly="True" />
                    <DataGridTextColumn Binding="{Binding Path=csv_error}" Header="Error" Width="*"
IsReadOnly="True" />
                </DataGrid.Columns>
            </DataGrid>
        </Grid>
    </Grid>

</Window>
